logs/
node_modules/
dist/
.DS_Store

# Environment files (use .env.example as template)
.env
.env.local
.env.*.local

# Claude Code local settings
.claude/settings.local.json

# TypeScript compiled files
*.js
*.d.ts
*.js.map
*.d.ts.map

# TypeScript build info
*.tsbuildinfo

# Keep source TypeScript files in examples (but not compiled .d.ts)
!examples/**/*.ts
!examples/**/*.tsx

# Exclude compiled .d.ts files in examples (they match *.ts pattern above)
examples/**/*.d.ts

# Keep root configuration files only
!/jest.config.js
!/webpack.config.js
!/scripts/*.js

# Keep package-level config files
!packages/**/jest.config.js

# Keep k6 load testing scripts (plain JS, not TypeScript compiled)
!tests/k6/**/*.js

# Keep example config files (source only, not compiled)
!examples/**/postcss.config.js
!examples/**/tailwind.config.js
!examples/**/vite.config.ts

# Keep example app source files (JSX/TSX are source, not compiled)
!examples/**/src/**/*.jsx
!examples/**/src/**/*.tsx

# Test coverage reports
coverage/
*.lcov
.nyc_output/

# k6 benchmark results (machine-specific)
tests/k6/results/

# V8 profiling output (machine-specific, regenerate with profile scripts)
profiles/
isolate-*.log
.clinic/

# Test Certificates (allow test fixtures)
# *.crt
# *.key
# packages/server/test/fixtures/

# Native addon build artifacts (regenerated by node-gyp, contain local paths)
packages/native/build/Makefile
packages/native/build/config.gypi
# Keep the compiled .node binary for distribution

# Custom k6 binary (platform-specific, rebuild with: pnpm test:k6:build)
bin/
