{
    "name": "topgun",
    "version": "0.0.0",
    "description": "Realtime, offline-first, secure, graph data synchronization engine. Reimplementation of gunDB in TypeScript",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup",
        "prepublishOnly": "pnpm run build",
        "test": "vitest run",
        "semantic-release": "semantic-release"
    },
    "browser": "dist/client.global.js",
    "exports": {
        "./client": {
            "types": "./dist/client.d.ts",
            "require": "./dist/client.js",
            "import": "./dist/client.mjs",
            "default": "./dist/client.js"
        },
        "./server": {
            "types": "./dist/server.d.ts",
            "require": "./dist/server.js",
            "import": "./dist/server.mjs",
            "default": "./dist/server.js"
        },
        "./package.json": "./package.json"
    },
    "typesVersions": {
        "*": {
            "client": [
                "./dist/client.d.ts"
            ],
            "server": [
                "./dist/server.d.ts"
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TopGunBuild/topgun.git"
    },
    "keywords": [
        "graph",
        "realtime",
        "database",
        "typescript",
        "javascript",
        "gun",
        "key-value store",
        "storage",
        "adapter",
        "performance"
    ],
    "author": "Ivan Kalashnik",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/TopGunBuild/topgun/issues"
    },
    "homepage": "https://github.com/TopGunBuild/topgun#readme",
    "dependencies": {
        "@peculiar/webcrypto": "^1.4.3",
        "ajv": "^8.12.0",
        "route-parser": "^0.0.5",
        "topgun-socket": "1.0.5"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^9.0.2",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^8.0.7",
        "@semantic-release/npm": "^10.0.3",
        "@semantic-release/release-notes-generator": "^10.0.3",
        "@types/route-parser": "^0.1.4",
        "semantic-release": "^21.0.1",
        "tsup": "^6.7.0",
        "typescript": "4.9.4",
        "vitest": "^0.29.8"
    }
}
