{
    "compilerOptions": {
        "target": "es2020",
        "module": "esnext",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "noUnusedLocals": true,
        "noImplicitAny": false,
        "allowJs": true,
        "noEmit": true,
        "outDir": "dist",
        "resolveJsonModule": true,

        "types": ["vitest/globals"]
    }
}
